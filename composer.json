{
    "name": "graphaware/neo4j-bolt",
    "description": "Neo4j Bolt Binary Protocol PHP Driver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "christophe@graphaware.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0",
        "graphaware/neo4j-common": "^3.4",
        "myclabs/php-enum": "^1.4"
    },
    "autoload": {
      "psr-4": {
          "GraphAware\\Bolt\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphAware\\Bolt\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/stopwatch": "^2.7",
        "behat/behat": "~3.0.4"
    },
    "minimum-stability": "dev"
}
